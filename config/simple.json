{
  "inputs": [
    {
      "Stdin": {
        "hostname": true,
        "type": "Stdin",
        "enableMeter": true,
        "codec": "plain"
      }
    },
    {
      "Redis": {
        "host": "localhost",
        "port": 6379,
        "channels": [
          "hangout",
          "tube"
        ],
        "type": "Redis",
        "enableMeter": true,
        "codec": "plain"
      }
    },
    {
      "File": {
        "codec": "json",
        "paths": [
          "D:\\temp\\log"
        ],
        "type": "Nginx",
        "encoding": "UTF8",
        "readPeriod": 5000,
        "threadNum": 5,
        "enableMeter": true,
        "startPosition": "beginning"
      }
    }
  ],
  "filters": [
    {
      "JavaScript": {
        "condition": "event['@fields'] ? true : false",
        "customFunctionPath": "D:\\tube\\config\\filter.js"
      },
      "Geoip2": {
        "condition": "event['remote_addr'] ? true : false",
        "ip": "remote_addr"
      },
      "Remove": {
        "fields": [
          "@fields",
          "@message"
        ]
      }
    }
  ],
  "outputs": [
    {
      "ElasticSearch": {
        "hosts": [
          "192.168.35.136:9200"
        ],
        "index": "tube-%{YYYY.MM.dd}",
        "documentType": "${type}",
        "enableMeter": true,
        "bulkSize": 300
      }
    }
  ]
}