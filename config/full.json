{
  "inputs": [
    {
      "Stdin": {
        "hostname": true,
        "type": "Stdin",
        "enableMeter": true,
        "codec": "plain"
      }
    },
    {
      "Redis": {
        "host": "localhost",
        "port": 6379,
        "password": "1234",
        "channels": ["hangout", "tube"],
        "type": "Microservices",
        "enableMeter": true,
        "codec": "json"
      }
    },
    {
      "File": {
        "codec": "plain",
        "paths": [
          "D:\\temp\\log\\access_20200610.log.gz",
          "D:\\temp\\log\\access.log"
        ],
        "type": "Nginx",
        "encoding": "UTF8",
        "startPosition": "beginning",
        "readPeriod": 3000,
        "enableMeter": true
      }
    }
  ],
  "filters": [
    {
      "Grok": {
        "patternPaths": [
          "/opt/hangout/grokpatternpaths"
        ],
        "matches": [
          "%{IP:client} %{WORD:method} %{URIPATHPARAM:request} %{NUMBER:bytes} %{NUMBER:duration}"
        ],
        "src": "@message",
        "enableMeter": true
      },
      "JavaScript": {
        "condition": "event['@fields'] ? true : false",
        "customFunctionPath": "D:\\tube\\config\\filter.js"
      },
      "Geoip2": {
        "condition": "event['remote_addr'] ? true : false",
        "ip": "remote_addr"
      },
      "Remove": {
        "fields": [
          "client",
          "@message"
        ]
      }
    }
  ],
  "outputs": [
    {
      "ElasticSearch": {
        "hosts": [
          "192.168.35.136:9200"
        ],
        "index": "tube-%{YYYY.MM.dd}",
        "documentType": "${type}",
        "enableMeter": true,
        "bulkSize": 300
      }
    },
    {
      "Stdout": {
        "enableMeter": true
      }
    },
    {
      "Redis": {
        "host": "localhost",
        "port": 6379,
        "channel": "hangout",
        "enableMeter": true
      }
    },
    {
      "Blackhole": {
        "enableMeter": true
      }
    }
  ]
}