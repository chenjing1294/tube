{
  "inputs": [
    {
      "File": {
        "codec": "json",
        "paths": [
          "D:\\temp\\log2"
        ],
        "type": "nginx",
        "encoding": "UTF8",
        "startPosition": "beginning",
        "readPeriod": 3000,
        "enableMeter": true
      }
    }
  ],
  "filters": [
    {
      "JavaScript": {
        "condition": "event['@fields'] ? true : false",
        "customFunctionPath": "D:\\tube\\config\\filter.js"
      },
      "Geoip2": {
        "condition": "event['remote_addr'] ? true : false",
        "ip": "remote_addr"
      },
      "Remove": {
        "fields": [
          "@fields",
          "@message"
        ]
      }
    }
  ],
  "outputs": [
    {
      "Stdout": {
        "enableMeter": true
      }
    }
  ]
}
