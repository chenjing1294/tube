{
  "inputs": [
    {
      "Redis": {
        "host": "localhost",
        "port": 6379,
        "channel": "hangout-output-redis",
        "type": "Redis",
        "enableMeter": true,
        "codec": "json"
      }
    },
    {
      "File": {
        "codec": "plain",
        "paths": [
          "D:\\temp\\log\\access_20200610.log.gz",
          "D:\\temp\\log\\access.log"
        ],
        "type": "Nginx",
        "encoding": "UTF8",
        "startPosition": "beginning",
        "period": 3000,
        "enableMeter": true
      }
    }
  ],
  "filters": [
    {
      "JavaScript": {
        "customFunctionPath": "D:\\playground\\tube\\conf\\filter.js"
      },
      "Remove": {
        "fields": [
          "@fields"
        ]
      }
    }
  ],
  "outputs": [
    {
      "Stdout": {
        "enableMeter": true
      }
    }
  ]
}